
MODULE main () 
VAR
	top-usr-OK : boolean;	top-res-subrange_1 : boolean;
ASSIGN
	init(top-usr-OK) := TRUE;
	init(top-res-subrange_1) := (0 <= (top-usr-OK ? 1 : 0) & (top-usr-OK ? 1 : 0) <= 1);

	next(top-usr-OK) := TRUE;
	next(top-res-subrange_1) := (0 <= (next(top-usr-OK) ? 1 : 0) & (next(top-usr-OK) ? 1 : 0) <= 1);


INVARSPEC 
